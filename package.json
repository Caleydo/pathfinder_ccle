{
  "name": "pathfinder_ccle",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/pathfinder_ccle",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_server": "*"
  },
  "scripts": {
    "postinstall": "./_scripts/setup.sh"
  },
  "caleydo": {
    "plugins": {
      "python": [
        {
          "type": "namespace",
          "file": "ccle",
          "namespace": "/api/ccle"
        },
        {
          "type": "json-encoder",
          "id": "dataset-hdf",
          "file": "json_encoder"
        }
      ]
    },
    "dependencies": {
      "python": {
        "numpy": ">=1.8.2",
        "tables": "==3.2.1.1",
        "python-memcached": "==1.57"
      },
      "debian": {
        "libhdf5-serial-dev": "",
        "memcached": ""
      },
      "redhat": {
        "hdf5-devel": ""
      }
    },
    "scripts": {
      "pre_start": "sudo service memcached start",
      "pre_start_redhat": "sudo service memcached start"
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
